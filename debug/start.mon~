reset
load --file=/home/unheil/pico/audiopwm/debug/audiopwm.hex
wrap -p 0 -s 0 -w 8 -t 0
gpio --pio=0 --gpio=0 --init
side -p 0 -s 0 -c 1 -b 0 +o
fifo -p 0 -s 0 --tx --join
fifo -p 0 -s 0 --tx --shift-right

sm -p 0 -s 0 +e
registers -p 0 -s 0 -a 6

fifo --enqueue --tx -v 0x0000ff00
fifo --enqueue --tx -v 0x0000ff00
fifo --enqueue --tx -v 0x000000ff
fifo --enqueue --tx -v 0x0000ff00
